{
  "name": "@mk-modular/shared",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "import": "./dist/events/index.js",
      "require": "./dist/events/index.cjs"
    },
    "./events/hooks": {
      "types": "./dist/events/hooks.d.ts",
      "import": "./dist/events/hooks.js",
      "require": "./dist/events/hooks.cjs"
    },
    "./events/types": {
      "types": "./dist/events/types.d.ts",
      "import": "./dist/events/types.js",
      "require": "./dist/events/types.cjs"
    },
    "./routing": {
      "types": "./dist/routing/index.d.ts",
      "import": "./dist/routing/index.js",
      "require": "./dist/routing/index.cjs"
    },
    "./components/Breadcrumbs": {
      "types": "./dist/components/Breadcrumbs.d.ts",
      "import": "./dist/components/Breadcrumbs.js",
      "require": "./dist/components/Breadcrumbs.cjs"
    },
    "./hooks/moduleLifecycle": {
      "types": "./dist/hooks/moduleLifecycle.d.ts",
      "import": "./dist/hooks/moduleLifecycle.js",
      "require": "./dist/hooks/moduleLifecycle.cjs"
    },
    "./middleware/auth": {
      "types": "./dist/middleware/auth.d.ts",
      "import": "./dist/middleware/auth.js",
      "require": "./dist/middleware/auth.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.20.5",
    "clsx": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
