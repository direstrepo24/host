{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": false,
    "declaration": true,
    "emitDeclarationOnly": true,
    "jsx": "react-jsx",
    "outDir": "dist",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true
  },
  "include": ["app/routes/_index.tsx"]
}
